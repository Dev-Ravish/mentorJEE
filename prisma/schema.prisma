generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mentor {
  id        String   @id @default(cuid())
  clerkId   String   @unique
  name      String?
  email     String?  @unique
  bio       String?
  image     String?
  oneLiner  String?
  upiId     String?
  tags      String[] 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
